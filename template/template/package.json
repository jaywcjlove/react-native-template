    "@rematch/core": "2.0.1",
    "@rematch/loading": "2.0.1",
    "@uiw/formatter": "1.2.4",
    "@uiw/react-native": "1.12.0",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-device-info": "8.1.3",
